<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<TargetFrameworks>netstandard2.0;netstandard2.1;net461</TargetFrameworks>
    <Nullable>disable</Nullable>
	<Version>1.0.1</Version>
    <Title>IocMapper</Title>
    <Authors>Simplelink</Authors>
    <Company>Simplelink</Company>
    <Copyright>2022</Copyright>
    <Description>A tool to simplify mapping interfaces and implementations for dependency injection and IOC.</Description>
    <RepositoryUrl>https://github.com/jwallace1983/IocMapper</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>Minor fixes and improvements, expanded target frameworks</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/jwallace1983/IocMapper</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

</Project>
